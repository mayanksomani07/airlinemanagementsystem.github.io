<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Scene5Controller">
   <children>
      <Label layoutX="234.0" layoutY="59.0" text="User Login">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="book" layoutX="235.0" layoutY="156.0" mnemonicParsing="false" onAction="#option" text="Book Ticket">
         <font>
            <Font size="14.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="user" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="cancel" layoutX="234.0" layoutY="190.0" mnemonicParsing="false" onAction="#option" text="Cancel Ticket" toggleGroup="$user">
         <font>
            <Font size="14.0" />
         </font>
      </RadioButton>
   </children>
</AnchorPane>
