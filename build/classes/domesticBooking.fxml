            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="from" layoutX="225.0" layoutY="143.0" prefWidth="150.0" />
      <ChoiceBox fx:id="to" layoutX="225.0" layoutY="197.0" prefWidth="150.0" />
      <DatePicker fx:id="date" layoutX="213.0" layoutY="255.0" />
      <Label layoutX="154.0" layoutY="146.0" text="From">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="171.0" layoutY="200.0" text="To">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="155.0" layoutY="258.0" text="Date">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="bookButton" layoutX="272.0" layoutY="325.0" mnemonicParsing="false" onAction="#book" text="BOOK">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="152.0" layoutY="95.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="226.0" layoutY="92.0" />
   </children>
</AnchorPane>
